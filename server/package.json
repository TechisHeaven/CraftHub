{
  "name": "server",
  "version": "1.0.0",
  "description": "crafthub server",
  "main": "index.js",
  "scripts": {
    "start": "nodemon src/index.ts",
    "build": "tsc",
    "lint": "node lint",
    "migrations:generate": "drizzle-kit generate:mysql",
    "migrations:push": "drizzle-kit push:mysql",
    "migrate": "ts-node --files drizzle/migrator.ts",
    "migrations:drop": "drizzle-kit drop --config=drizzle.config.ts"
  },
  "author": "himanshu",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.29.4",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.9.2",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "uuid4": "^2.0.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.23",
    "@types/uuid4": "^2.0.3",
    "drizzle-kit": "^0.20.14",
    "typescript": "^5.4.2"
  }
}
